{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/appIcons/1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.frontendapp"
    },
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./app/assets/appIcons/1024.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.frontendapp"
    },
    "name": "frontend-app",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/appIcons/1024.png",
          "resizeMode": "stretch",
          "backgroundColor": "#ffffff",
          "imageWidth": "500%"
        }
      ],
      "expo-font"
    ],
    "scheme": "myapp",
    "slug": "frontend-app",
    "userInterfaceStyle": "automatic",
    "version": "1.0.0"
  }
}
